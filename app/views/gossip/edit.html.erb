<h1>Edit a gossip</h1>


<%= form_for @gossip do |f| %>

<div class="form-group">
  <%= f.text_field :title, class: 'form-control' %>
</div>

<div class="form-group">
  <%= f.text_area :content, class: 'form-control' %>
</div>

<div class="form-group">
	<button>Edit gossip</button>
</div>

<% end %>